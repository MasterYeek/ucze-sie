{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss Natours.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss Natours.comp.css",
    "concat:css": "concat -o sass/Natours.concat.css css/icon-font.css Natours.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' sass/Natours.concat.css -o css/prefix.css",
    "compress:css": "node-sass css/prefix.css css/Natours.css --output-style compressed",
    "build:css": "npm-run-all compile:sass  concat:css  prefix:css  compress:css"
  },
  "author": "Tom",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  },
  "dependencies": {
    "live-server": "^1.2.2"
  }
}
